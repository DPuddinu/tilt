---
type Props = {
  height?: number;
  width?: number;
};
const { height = 24, width = 24 } = Astro.props;
---

<svg
  class='sm:h-8 sm:w-8 md:h-14 md:w-14'
  xmlns='http://www.w3.org/2000/svg'
  width={width}
  height={height}
  viewBox='0 0 24 24'
  fill='none'
  stroke='currentColor'>
  <slot />
</svg>
